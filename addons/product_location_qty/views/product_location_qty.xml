<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_product_template_inherit_view" model="ir.ui.view">
			<field name="name">Product Template View</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_tree_view"/>
			<field name="arch" type="xml">
				<field name="categ_id" position="after">
						<field name="str_qty" />
						<field name="kaz_qty"/>
                        <field name="led_qty"/>
				</field>
			</field>
		</record>

        <record id="product_template_tree_view" model="ir.ui.view">
            <field name="name">Product Template Tree View</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <field name="uom_id" position="after">
                    <field name="str_qty"/>
                    <field name="kaz_qty"/>
                    <field name="led_qty"/>
                </field>
            </field>
        </record>

        <record id="product_product_inherit_view" model="ir.ui.view">
			<field name="name">Product Product View</field>
			<field name="model">product.product</field>
			<field name="inherit_id" ref="product.product_product_tree_view"/>
			<field name="arch" type="xml">
				<field name="categ_id" position="after">
						<field name="str_qty" />
						<field name="kaz_qty"/>
                        <field name="led_qty"/>
				</field>
			</field>
		</record>

         <record id="product_product_tree_view" model="ir.ui.view">
            <field name="name">Product Product Tree View</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <field name="barcode" position="after">
                    <field name="str_qty"/>
                    <field name="kaz_qty"/>
                    <field name="led_qty"/>
                </field>
            </field>
        </record>

        <record id="view_product_product_inherit_view" model="ir.ui.view">
			<field name="name">Stock Location Tree View</field>
			<field name="model">stock.location</field>
			<field name="inherit_id" ref="stock.view_location_form"/>
			<field name="arch" type="xml">
				<field name="location_id" position="after">
                        <field name="customise_name" />
				</field>
			</field>
		</record>

    </data>
</openerp>
